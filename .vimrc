set number
set expandtab
set tabstop=4
set shiftwidth=4
set softtabstop=4

colo elflord

syntax on

set wrap

set path=**

set wildmenu



" vim plug
call plug#begin('~/.vim/plugged')
Plug 'https://github.com/preservim/nerdtree'
Plug 'https://github.com/junegunn/fzf'
Plug 'https://github.com/frazrepo/vim-rainbow'
call plug#end()
